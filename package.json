{
  "name": "formula-1-season-standings",
  "version": "2.2.1",
  "description": "",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "-": "",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "commit": "git-cz",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:write": "tslint -c tslint.json 'src/**/*.{ts,tsx}' --fix",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "start:cra": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "jest --coverage",
    "test:watch": "./node_modules/.bin/jest --watch",
    "eject": "react-scripts eject"
  },
  "author": "Pavel \"Ckomop0x\" Klochkov <paul.klochkov@gmail.com> (https://ckomop0x.github.io/)",
  "license": "ISC",
  "dependencies": {
    "isomorphic-unfetch": "3.0.0",
    "nprogress": "^0.2.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "styled-components": "^4.3.2",
    "typescript": "^3.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/nprogress": "0.0.29",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-test-renderer": "^16.8.1",
    "@types/styled-components": "^4.1.19",
    "autoprefixer": "^9.2.1",
    "commitizen": "3.1.1",
    "css-loader": "^1.0.0",
    "customize-cra": "0.6.0",
    "cypress": "3.3.1",
    "cz-conventional-changelog": "2.1.0",
    "ejs-loader": "0.3.3",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "gh-pages": "^2.0.1",
    "ghooks": "2.0.4",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-custom-media": "^7.0.7",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.15.2",
    "raw-loader": "3.1.0",
    "react-app-rewire-styled-components": "3.0.2",
    "react-app-rewired": "2.1.3",
    "react-dev-utils": "9.0.3",
    "react-scripts": "3.1.1",
    "react-test-renderer": "^16.6.0",
    "semantic-release": "15.13.14",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.2.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-import-rules": "^0.3.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typed-css-modules": "^0.3.6",
    "typings-for-css-modules-loader": "^1.7.0",
    "uglifyjs-webpack-plugin": "2.1.3",
    "url-loader": "^1.1.2",
    "webpack-bundle-analyzer": "3.4.1",
    "webpack-log": "3.0.1"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "test/coverage",
    "coverageReporters": [
      "cobertura",
      "html"
    ],
    "collectCoverageFrom": [
      "src/scripts/*.{ts,tsx}",
      "src/scripts/**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!src/scripts/**/__tests__/*.spec.{ts,tsx}",
      "!src/scripts/**/*.spec.{ts,tsx}",
      "!src/scripts/*.d.ts",
      "!src/scripts/**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": "<rootDir>/test-preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "^.+\\.(d.ts)$"
    ],
    "testRegex": "/__tests__/.*\\.(ts|tsx|js|jsx)$"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
