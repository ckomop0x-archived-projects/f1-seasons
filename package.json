{
  "name": "formula-1-season-standings",
  "version": "2.2.3",
  "description": "",
  "scripts": {
    "dev": "next dev -p 8100",
    "build": "next build",
    "start": "next start",
    "cra:start": "react-scripts start",
    "cra:build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:rewired": "react-app-rewired start",
    "build:rewired": "react-app-rewired build",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "author": "Pavel \"Ckomop0x\" Klochkov <paul.klochkov@gmail.com> (https://ckomop0x.github.io/)",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@emotion/babel-plugin": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "date-fns": "^2.29.3",
    "eslint-config-next": "^13.1.5",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-react-parser": "^3.0.8",
    "jest-environment-jsdom": "^29.4.0",
    "next": "^13.1.6",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.2.1",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/nprogress": "0.2.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.7",
    "css-loader": "^1.0.0",
    "customize-cra": "0.6.0",
    "cypress": "12.4.1",
    "eslint": "^8.32.0",
    "husky": "^8.0.3",
    "jest": "^29.4.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "test/coverage",
    "coverageReporters": [
      "cobertura",
      "html"
    ],
    "collectCoverageFrom": [
      "src/scripts/*.{ts,tsx}",
      "src/scripts/**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!src/scripts/**/__tests__/*.spec.{ts,tsx}",
      "!src/scripts/**/*.spec.{ts,tsx}",
      "!src/scripts/*.d.ts",
      "!src/scripts/**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": "<rootDir>/test-preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "^.+\\.(d.ts)$"
    ],
    "testRegex": "/__tests__/.*\\.(ts|tsx|js|jsx)$"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
